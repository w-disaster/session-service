{
  "name": "session-service",
  "description": "Session Service of LetsStreamIt",
  "keywords": [
    "typescript",
    "template"
  ],
  "author": {
    "name": "LetsStreamIt",
    "url": "https://github.com/LetsStreamIt"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/LetsStreamIt/session-service"
  },
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "node -e \"var { rmdirSync, existsSync } = require('fs'), path = require('path'); ['./dist'].forEach(fPath => {if (existsSync(path.join(__dirname, fPath))) rmdirSync(path.join(__dirname, fPath), { recursive: true })}); process.exit(0);\"",
    "clean:all": "npm run clean && (rm -r ./node_modules || true)",
    "lint:fix": "eslint --fix src/**/*.ts test/**/*.ts",
    "lint:check": "eslint src/**/*.ts test/**/*.ts",
    "docs:build": "typedoc --entryPointStrategy expand ./src",
    "format:write": "prettier --write src/ test/",
    "format:check": "prettier --check src/ test/",
    "preversion": "npm run build && npm run cover:check",
    "prepare": "husky",
    "start": "node dist/app.js",
    "test": "mocha && depcruise src",
    "debug": "node --inspect ./src/app.ts"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.4.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@eslint/js": "^9.8.0",
    "@types/chai": "^5.0.0",
    "@types/eslint__js": "^8.42.3",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/mocha": "^10.0.9",
    "@types/node": "^22.0.0",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "@vue/eslint-config-prettier": "^10.0.0",
    "chai": "^4.5.0",
    "dependency-cruiser": "^16.5.0",
    "eslint": "^9.8.0",
    "husky": "^9.1.4",
    "lint-staged": "^15.2.8",
    "mocha": "^10.2.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.2.5",
    "semantic-release-preconfigured-conventional-commits": "1.1.112",
    "source-map-support": "^0.5.21",
    "ts-node": "^10.9.1",
    "typedoc": "^0.26.10",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.0.0"
  },
  "engines": {
    "node": "20.18"
  },
  "dependencies": {
    "@types/socket.io": "^3.0.2",
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "js-sha256": "^0.11.0",
    "socket.io": "^4.8.0",
    "socket.io-client": "^4.8.0"
  }
}
